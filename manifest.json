{
  "manifest_version": 2,
  "name": "Materialistic Me",
  "description": "<description_of_the_chrome_plugin>",
  "version": "0.1",
  "permissions": [],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  /*"chrome_url_overrides": {
    "newtab": "app/index.html"
  },*/
  "icons": {
    "128": "app/img/bag-icon-128.png",
    "48": "app/img/bag-icon-48.png",
    "16": "app/img/bag-icon-16.png"
  },
  "browser_action": {
    "default_icon": "app/img/bag-icon-48.png",
    "default_popup": "app/index.html"
  }
}